[Unit]
Description=Gunicorn instance to serve flask + gunicorn
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v3/AirBnB_clone_v4
Environment="PATH=/home/ubuntu/.local/bin/:/usr/bin/"
ExecStart=/home/ubuntu/.local/bin/gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app
StandardError=file:/tmp/airbnb-error.log
StandardOutput=file:/tmp/airbnb-access.log

[Install]
WantedBy=multi-user.target
